<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dbprosjekt.controllers.SubjectController">
    <children>
        <Text layoutX="100" layoutY="100">Create a new subject</Text>
        <TextField layoutX="100" layoutY="200" fx:id="subjectNameInput" promptText="Subject name"/>
        <TextField layoutX="100" layoutY="300" fx:id="subjectIdInput" promptText="Subject ID"/>
        <Button layoutX="100" layoutY="300" onAction="#createSubject">Create folder</Button>
        <Text layoutX="100" layoutY="350" fx:id="errorMessage"/>
    </children>
</AnchorPane>
